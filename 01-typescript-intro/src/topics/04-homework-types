interface SuperHero {
    name: string;
    age: number;
    address: Address;
    showAddress: () => string;
}
interface Address {
    street: string;
    city: string;
    country: string;
}

const superHeroe: SuperHero = {
    name: 'Spiderman',
    age: 30,
    address: {
        street: 'Main St',
        city: 'USA',
        country: 'NY'
    },
    showAddress() {
        return this.name + ', ' + this.address.ciudad + ', ' + this.address.pais;
    }
}


const address = superHeroe.showAddress();
// console.log( address );




export {};